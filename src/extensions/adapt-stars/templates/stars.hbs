<div class="stars-inner">
    <p>{{this}}</p>

{{#if minimalUi}}
	<div class="stars-count icon">
        {{score}}
    </div>
    <div class="icon icon-star">
    </div>
{{else}}
    {{#each stars}}
    <div class="icon {{#if _isCorrect}}icon-star{{else}}icon-star-hollow{{/if}}">
    </div>
    {{/each}}
{{/if}}

    
</div>